{
  "env": {
    "NODE_ENV": "staging"
  },
  "terminateTimeout": 30000,
  "processes": [
    {
      "name": "Ping Localhost",
      "cmd": "ping",
      "args": ["localhost"],
      "instances": 3
    },
    {
      "name": "Ping 127.0.0.1",
      "cmd": "ping",
      "args": ["127.0.0.1"],
      "instances": 1
    },
    {
      "name": "Ping 127.0.0.1",
      "cmd": "ping",
      "args": ["127.0.0.1"],
      "env": {
        "LOGFILE": "example.log"
      },
      "instances": 1
    },
    {
      "name": "Ping Google",
      "cmd": "ping",
      "args": ["google.com"],
      "instances": 0
    }
  ]
}