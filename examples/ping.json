{
  "env": {
    "NODE_ENV": "staging"
  },
  "killTimeoutMs": 30000,
  "processes": [
    {
      "name": "Ping Localhost",
      "cmd": "ping",
      "args": ["localhost"],
      "instances": 2
    },
    {
      "name": "Ping 127.0.0.1",
      "cmd": "ping",
      "args": ["127.0.0.1"],
      "instances": 1,
      "autoRestart": true,
      "autoRestartTimeoutMs": 3000,
    },
    {
      "name": "Ping 127.0.0.1",
      "cmd": "ping",
      "args": ["127.0.0.1"],
      "env": {
        "MYVAR": "123"
      },
      "instances": 1
    },
    {
      "name": "Ping Google",
      "cmd": "ping",
      "args": ["google.com"],
      "instances": 0
    }
  ]
}